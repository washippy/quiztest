package {     import flash.events.*;    import flash.net.*;    import flash.display.*;        import flash.text.TextField;    public class XMLParser extends EventDispatcher{                private var dataChunk:XML;        private var urlLoader:URLLoader;        public var dataString:String;        public var XMLPATHSTRING:String;        public var xmlLoaded:Boolean;   		public static const DATA_READY:String = "DATA_READY";             public function XMLParser(){			trace("XXXMMMLLL");		}                public function doIt(_XMLPATHSTRING:String):void{			trace("DOIN IT " +_XMLPATHSTRING)			XMLPATHSTRING = _XMLPATHSTRING;			var urlRequest:URLRequest=new URLRequest(XMLPATHSTRING);			urlLoader = new URLLoader();			urlLoader.addEventListener(Event.COMPLETE, completeListener);			urlLoader.load(urlRequest);        }		        public function getIt():XML{			return dataChunk;		}        private function completeListener(e:Event):void{            dataChunk = new XML(urlLoader.data);			xmlLoaded = true;			dispatchEvent(new Event(XMLParser.DATA_READY));		//	trace("DONE "+dataChunk);        }                private function progressHandler(event:ProgressEvent):void {                 trace("XML progressHandler loaded:" + event.bytesLoaded + " total: " + event.bytesTotal);        }	}}